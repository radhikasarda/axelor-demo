            <xml-property name="queryText"><![CDATA[select employee.full_name as fullName,
metaFile.file_path as image_path,
employee.email as email,
gender as gender,phone.contact_number as contact
from employee_details_employee as employee 
Left join meta_file as metaFile 
on employee.image=metaFile.id
Left join employee_details_phone as phone
on employee.id=phone.employee
where employee.id = 
any (string_to_array(?, ',')::int[])]]></xml-property>
